{
  "name": "family-tree",
  "version": "1.0.0",
  "description": "Using D3 v6 to draw a fancy family tree",
  "main": "lib/family-tree.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-register -r test/setup.js --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register -r test/setup.js --colors -w ./test/*.spec.js",
    "test:cover": "cross-env NODE_ENV=test nyc mocha --require babel-register -r test/setup.js --colors ./test/*.js",
    "repl": "node -i -e \"$(< ./lib/family-tree.js)\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zuopingli/family-tree.git"
  },
  "keywords": [
    "webpack",
    "es6",
    "d3 tree",
    "family tree",
    "hierarchy tree",
    "umd",
    "commonjs"
  ],
  "author": "Zuoping Li",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zuopingli/family-tree/issues"
  },
  "homepage": "https://github.com/zuopingli/family-tree",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "jsdom": "^11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^4.0.1",
    "nyc": "^13.1.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.0.8",
    "yargs": "^10.0.3"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "dependencies": {
    "chai-spies": "^1.0.0",
    "d3": "^5.7.0"
  }
}
